<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>

	<context id="entmobile" targetRuntime="MyBatis3">
		<plugin type="org.mybatis.generator.plugins.ToStringPlugin"/>
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin"></plugin>

		<commentGenerator>
			<property name="suppressDate" value="true" />
			<!-- remove comments -->
			<property name="suppressAllComments" value="false" />
		</commentGenerator>

		<!-- jdbc driver 用户名 密码 数据库名 的配置-->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
						connectionURL="jdbc:mysql://127.0.0.1:3306/fanli?useUnicode=true&amp;characterEncoding=utf8&amp;allowMultiQueries=true&amp;serverTimezone=UTC"
						userId="root" password="111111" />

		<!-- java type resolver -->
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- gem entity -->
		<!-- data -->
		<!--在此修改生成路径-->
		<javaModelGenerator targetPackage="com.datu.fanli.member.bean"
							targetProject="./src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="false" />
			<property name="constructorBased" value="true"/>
		</javaModelGenerator>

		<sqlMapGenerator targetPackage ="mapper"
						 targetProject ="./src/main/resources">
			<property name ="enableSubPackages" value =" true" />
		</sqlMapGenerator>


		<!-- gem annotated mapper -->
		<!-- data -->
		<!--在此修改生成路径-->
		<javaClientGenerator type="XMLMAPPER"
							 targetPackage="com.datu.fanli.member.dao"
							 targetProject="./src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>





		<!--要生成的表-->
		<table tableName="ums_member" domainObjectName="Member">
			<generatedKey column="id" sqlStatement="JDBC"/>
			<columnOverride column="id" javaType="String"/>
			<columnOverride column="member_level_id" javaType="String"/>
		</table>



	</context>
</generatorConfiguration>